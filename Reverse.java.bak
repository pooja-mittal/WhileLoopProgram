import java.util.Scanner;
class Reverse
{
	public static void main(String[] args)
	{
		Scanner scr=new Scanner(System.in);
		System.out.println("enter one number");
		int n=scr.nextInt();
		int m=0; 
		while(n>0) {
			m=(m*10)+(n%10);
			n /= 10;
		}
		System.out.println("reverse of a number: " + m);
	}
}